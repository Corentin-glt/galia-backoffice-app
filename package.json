{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "codegen": "apollo client:codegen --customScalarsPrefix=GraphQL --globalTypesFile ./src/__generated__/globalTypes.ts --passthroughCustomScalars --target typescript && prettier --loglevel silent --write \"src/**/__generated__/*.ts\"",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "serve": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "~3.4.17",
    "@headlessui/react": "^1.4.2",
    "graphql": "^15.7.2",
    "prettier": "^2.5.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.0.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-react": "^1.0.0",
    "apollo": "^2.33.9",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "postcss": "^8.4.4",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "@apollo/client": "~3.4.17"
  }
}
